{
  "name": "mindreel",
  "productName": "MindReel",
  "version": "1.0.0",
  "description": "Daily Logs. AI Highlights.",
  "main": ".vite/build/main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "make:mac-intel": "electron-forge make --platform=darwin --arch=x64",
    "make:mac-arm": "electron-forge make --platform=darwin --arch=arm64",
    "make:all": "npm run make:mac-intel && npm run make:mac-arm",
    "publish": "electron-forge publish",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format .",
    "format:write": "biome format --write .",
    "validate": "npm run typecheck && npm run lint",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:reset": "supabase db reset",
    "supabase:push": "supabase db push",
    "supabase:functions": "supabase functions serve --env-file ./supabase/.env.local",
    "build:landing": "node scripts/build-landing.mjs --minify",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major"
  },
  "keywords": [],
  "author": "zetdotcom",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "2.2.5",
    "@electron-forge/cli": "^7.9.0",
    "@electron-forge/maker-dmg": "^7.10.2",
    "@electron-forge/maker-zip": "^7.9.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.9.0",
    "@electron-forge/plugin-fuses": "^7.9.0",
    "@electron-forge/plugin-vite": "^7.9.0",
    "@electron-forge/publisher-github": "^7.10.2",
    "@electron/fuses": "^1.8.0",
    "@playwright/test": "^1.56.1",
    "@tailwindcss/postcss": "^4.0.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/electron-squirrel-startup": "^1.0.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "^4.0.4",
    "@vitest/ui": "^4.0.4",
    "electron": "^38.2.2",
    "happy-dom": "^20.0.8",
    "jsdom": "^27.0.1",
    "playwright": "^1.56.1",
    "supabase": "^2.48.3",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.9.3",
    "vite": "^5.4.20",
    "vitest": "^4.0.4"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@supabase/supabase-js": "^2.75.0",
    "@types/sqlite3": "^3.1.11",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "electron-squirrel-startup": "^1.0.1",
    "lucide-react": "^0.545.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router": "^7.9.4",
    "sqlite3": "^5.1.7",
    "tailwind-merge": "^3.3.1",
    "tw-animate-css": "^1.4.0"
  }
}
